{
  "name": "React-Native-Paper-Project",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watch --coverage=false --detectOpenHandles",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@react-native-community/slider": "^4.5.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "expo": "^51.0.21",
    "expo-image-picker": "~15.0.7",
    "expo-local-authentication": "~14.0.1",
    "expo-status-bar": "~1.12.1",
    "express": "^4.19.2",
    "node": "^21.3.0",
    "qs": "^6.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.74.3",
    "react-native-add-wallet": "^0.1.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-chart": "^1.0.8-beta",
    "react-native-chart-kit": "^6.12.0",
    "react-native-date-picker": "^5.0.4",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-image-picker": "^7.1.2",
    "react-native-pager-view": "^6.3.0",
    "react-native-paper": "^5.12.3",
    "react-native-paper-dates": "^0.22.19",
    "react-native-reanimated": "^3.10.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-segmented-control-tab": "^4.0.0",
    "react-native-svg": "^15.2.0",
    "react-native-swipeable": "^0.6.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-wallet-manager": "^0.0.7",
    "react-navigation": "^5.0.0",
    "react-router-dom": "^6.24.0",
    "swipeable-react": "^2.0.0",
    "swiper": "^11.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react-native": "^12.5.2",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3"
  },
  "private": true
}
